
  {
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "http://127.0.0.1:5000/*"  
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "image.png",
        "48": "image.png",
        "128": "image.png"
      }
    }
  }